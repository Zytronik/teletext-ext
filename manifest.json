{
  "name": "Teletext Converter",
  "version": "0.0.1",
  "description": "Diese Extension verwandelt Webseiten in Teletext.",
  "manifest_version": 2,
  "icons": {
    "201": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/js/jquery.min.js",
        "/js/pixelit.js",
        "/js/main.js"
      ]
    }
  ]
}